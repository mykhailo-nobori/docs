{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Nobori.ai - Docs",
  "head": "<link rel=\"stylesheet\" href=\"/docs.css\" />",
  "colors": {
    "primary": "#FF5E00",
    "light": "#FF833D",
    "dark": "#CC4B00"
  },
  "favicon": "/nobori-favicon-dark-32.png",
  "logo": {
    "light": "/logo/nobori-logo-dark (2).png",
    "dark": "/logo/nobori-logo-light (2).png"
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "icon": "globe",
          "href": "https://nobori.ai"
        },
        {
          "anchor": "App",
          "icon": "square-arrow-up-right",
          "href": "https://app.nobori.ai"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get started",
            "pages": [
              "index",
              "quickstart",
              "sidebar-navigation"
            ]
          },
          {
            "group": "Track",
            "pages": [
              "dashboard",
              "prompts",
              "prompt-details",
              "competitors"
            ]
          },
          {
            "group": "Measure",
            "pages": [
              "citations"
            ]
          },
          {
            "group": "Get Cited",
            "pages": [
              "foundation",
              "cluster-actions"
            ]
          }
        ]
      },
      {
        "tab": "API",
        "hidden": true,
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api/overview"
            ]
          },
          {
            "group": "Coming soon",
            "pages": [
              "api/status",
              "api/webhooks"
            ]
          }
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@nobori.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Go to Dashboard",
      "href": "https://app.nobori.ai"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/nobori_ai",
      "linkedin": "https://linkedin.com/company/nobori"
    }
  }
}